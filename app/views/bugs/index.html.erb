

<h1>Bugs</h1>

<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>

      <th>Title</th>
      <th>Status</th>
    </tr>
  </thead>

    <% @bugs.each do |bug| %>
      <tr>      
        <td><%= link_to bug.title ,bug  %></td>
        <td><%= bug.status %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

<br>
